@model Vaccine
@{
    var lines = Model?.Description?
        .Split(new char[] {'\n'}, StringSplitOptions.RemoveEmptyEntries)
        .Where(t=>!String.IsNullOrWhiteSpace(t))
        .ToArray();
}

@if (lines?.Length > 0)
{
    <ul>
        @foreach (var line in lines)
        {
            <li>@(line)</li>
        }
    </ul>
}